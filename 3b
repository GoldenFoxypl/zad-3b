Zad. 8
            double w = int.Parse(Console.ReadLine());
            int l = int.Parse(Console.ReadLine());
            double proc = 0;
            for (int i = 0; i < l; i++)
            {
                proc = 0.06 * w;
                w += proc;
            }
            Console.WriteLine(Math.Round(w));
            Console.ReadKey();
